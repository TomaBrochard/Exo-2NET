<Page x:Class="testWPF.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:testWPF"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="MainPage">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MyDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>


    <Canvas >
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left"
            Margin="111,100,0,0" VerticalAlignment="Top" Width="75"
            Template="{StaticResource MyTemplate}" />
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Left"
            Margin="0,100,0,0" VerticalAlignment="Top" Width="75" />
    </Canvas>
    



</Page>
